<?xml version="1.0" encoding="utf-8" ?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SlippiTV.Client.Resources.Styles.GCPurpleTheme">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="Colors.xaml"/>
        <ResourceDictionary Source="Styles.xaml"/>
    </ResourceDictionary.MergedDictionaries>
    
    <Color x:Key="Primary">#140E4C</Color>
    <Color x:Key="PrimaryDark">#acAAea</Color>
    <Color x:Key="PrimaryLight">#898FFF</Color>
    <Color x:Key="PrimaryText">#FAFAFA</Color>
    <Color x:Key="SecondaryText">#CACACA</Color>
    <Color x:Key="PrimaryDarkText">#2B0B98</Color>
    <Color x:Key="Secondary">#0D004E</Color>
    <Color x:Key="AccentButtons">#0D004E</Color>
    <Color x:Key="SecondaryDark">#3F3FFF</Color>
    <Color x:Key="Tertiary">#2F2FFF</Color>
    <Color x:Key="Quatriary">#0D0125</Color>
</ResourceDictionary>